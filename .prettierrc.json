{
  "trailingComma": "none",
  "endOfLine": "auto",
  "tabWidth": 2,
  "semi": false,
  "singleQuote": true,
  "arrowParens": "always",
  "tailwindAttributes": [
    "iconClassName",
    "maskClassName",
    "titleClassName",
    "wrapperClassName"
  ],
  "tailwindFunctions": ["clsx", "cn"],
  "plugins": ["prettier-plugin-tailwindcss"]
}
